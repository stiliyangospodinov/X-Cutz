{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)", 
      "dest": "https://x-cutz.onrender.com/admin/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ]
}
